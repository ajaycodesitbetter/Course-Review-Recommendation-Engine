{
  "tech_stack": [
    "Python",
    "FastAPI",
    "pandas", 
    "numpy",
    "scikit-learn",
    "aiohttp",
    "TF-IDF Vectorizer",
    "Cosine Similarity"
  ],
  "features": [
    {
      "name": "API Status",
      "description": "Health check endpoint for API status",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/api": {
            "get": {
              "summary": "API health check",
              "responses": {
                "200": {
                  "description": "API status response",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "message": {"type": "string"},
                          "status": {"type": "string"},
                          "created_by": {"type": "string"}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Course Search",
      "description": "Search courses by query string using local dataset",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/search": {
            "get": {
              "summary": "Search for courses",
              "parameters": [
                {
                  "name": "query",
                  "in": "query",
                  "required": true,
                  "schema": {"type": "string"},
                  "description": "Search query for courses"
                }
              ],
              "responses": {
                "200": {
                  "description": "List of matching courses",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "integer"},
                            "title": {"type": "string"},
                            "visible_instructors": {"type": "array"},
                            "image_480x270": {"type": "string"},
                            "price": {"type": "string"},
                            "is_paid": {"type": "boolean"},
                            "avg_rating": {"type": "number"},
                            "rating": {"type": "number"},
                            "num_subscribers": {"type": "integer"},
                            "num_reviews": {"type": "integer"},
                            "instructional_level": {"type": "string"},
                            "headline": {"type": "string"},
                            "description": {"type": "string"},
                            "primary_category": {"type": "object"},
                            "url": {"type": "string"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Course Recommendations",
      "description": "AI-powered course recommendations based on similarity using embeddings",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/recommendations": {
            "get": {
              "summary": "Get course recommendations based on a course ID",
              "parameters": [
                {
                  "name": "course_id",
                  "in": "query",
                  "required": true,
                  "schema": {"type": "integer"},
                  "description": "Course ID to get recommendations for"
                },
                {
                  "name": "limit",
                  "in": "query",
                  "required": false,
                  "schema": {"type": "integer", "minimum": 1, "maximum": 50, "default": 10},
                  "description": "Maximum number of recommendations"
                }
              ],
              "responses": {
                "200": {
                  "description": "List of recommended courses",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "integer"},
                            "title": {"type": "string"},
                            "visible_instructors": {"type": "array"},
                            "image_480x270": {"type": "string"},
                            "price": {"type": "string"},
                            "is_paid": {"type": "boolean"},
                            "avg_rating": {"type": "number"},
                            "rating": {"type": "number"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Trending Courses",
      "description": "Get trending courses based on subscriber count with caching",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/trending": {
            "get": {
              "summary": "Get trending courses",
              "parameters": [
                {
                  "name": "limit",
                  "in": "query",
                  "required": false,
                  "schema": {"type": "integer", "minimum": 1, "maximum": 50, "default": 10},
                  "description": "Maximum number of courses to return"
                }
              ],
              "responses": {
                "200": {
                  "description": "List of trending courses with cache headers",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "integer"},
                            "title": {"type": "string"},
                            "visible_instructors": {"type": "array"},
                            "num_subscribers": {"type": "integer"},
                            "rating": {"type": "number"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Top Rated Courses",
      "description": "Get top-rated courses with minimum review threshold and caching",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/top-rated": {
            "get": {
              "summary": "Get top-rated courses",
              "parameters": [
                {
                  "name": "limit",
                  "in": "query",
                  "required": false,
                  "schema": {"type": "integer", "minimum": 1, "maximum": 50, "default": 10},
                  "description": "Maximum number of courses to return"
                }
              ],
              "responses": {
                "200": {
                  "description": "List of top-rated courses with cache headers",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "integer"},
                            "title": {"type": "string"},
                            "rating": {"type": "number"},
                            "num_reviews": {"type": "integer"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Course Categories",
      "description": "Get available course categories for filtering",
      "files": ["main.py"],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/categories": {
            "get": {
              "summary": "Get available course categories",
              "responses": {
                "200": {
                  "description": "List of available course categories",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "string"},
                            "name": {"type": "string"},
                            "icon": {"type": "string"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  ]
}